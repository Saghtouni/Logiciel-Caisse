                  <Font size="22.0" />
               </font>
            </Button>
            <Button fx:id="btnAnnuler" layoutX="280.0" layoutY="113.0" mnemonicParsing="false" onMouseClicked="#btnAnnulerClicked" prefHeight="57.0" prefWidth="161.0" styleClass="ipad-grey" stylesheets="@Fenetre.css" text="Annuler">
