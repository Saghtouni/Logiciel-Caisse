<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <AnchorPane prefHeight="196.0" prefWidth="460.0" styleClass="CAISSE-FINAL" stylesheets="@Fenetre.css">
